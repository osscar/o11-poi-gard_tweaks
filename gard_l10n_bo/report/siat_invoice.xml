<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data>

    <record id="account.account_invoices" model="ir.actions.report">
      <field name="paperformat_id" ref="poi_x_gard.paperformat_euro_pos_gard_ticket" />
      <field name="attachment_use" eval="False" />
    </record>

    <template id="siat_invoice">
      <t t-call="web.basic_layout">
        <t t-foreach="docs" t-as="o">

          <div class="page">
            <!-- row 1: water mark -->
            <div id="page_row1">
              <t t-if="'piloto' in o.siat_link">
                <div id="watermark1" class="text-center"
                  style="font-size: 80pt; position:absolute; opacity:0.25;">
                  <br />
                  <span>SIN VALOR LEGAL<br />
                    . . . . . . . . . . . . . . . . . . . . . . 
                  </span>
                  <br />
                  <span>SIN VALOR LEGAL<br />
                    . . . . . . . . . . . . . . . . . . . . . . 
                  </span>
                  <br />
                  <span>SIN VALOR LEGAL<br />
                    . . . . . . . . . . . . . . . . . . . . . . 
                  </span>
                  <br />
                  <span>SIN VALOR LEGAL<br />
                    . . . . . . . . . . . . . . . . . . . . . . 
                  </span>
                  <br />
                </div>
                  
              </t>
              <t t-if="o.siat_state == 'anulada'">
                <div id="watermark2" class="tex-center"
                  style="font-size: 60pt; margin-top: 250pt; margin-left:-500pt; color: red; position:absolute;
                  opacity:0.55; z-index:10000; transform:rotate(325deg); -webkit-transform:rotate(325deg); width:350%;">
                  <span>A N U L A D A . . ANULADA . A N U L A D A</span>
                  <br />
                  <span>ULADA . ANULADA . A N U L A D A . ANULADA . A N U L A D A . </span>
                  <br />
                  <span>. ANULADA . A N U L A D A . ANULADA</span>
                  <br />
                  <span>ULADA . ANULADA . A N U L A D A . ANULADA . A N U L A D A . </span>
                  <br />
                  <span>A N U L A D A . . ANULADA . A N U L A D A</span>
                  <br />
                  <span>ULADA . ANULADA . A N U L A D A . ANULADA . A N U L A D A . </span>
                  <br />
                  <span>. ANULADA . A N U L A D A . ANULADA</span>
                  <br />
                  <span>ULADA . ANULADA . A N U L A D A . ANULADA . A N U L A D A . </span>
                </div>
              </t>
            </div>

            <!-- row 2: invoice type A -->
            <div id="page_row2" class="text-center" style="font-size: 26px">
              <strong>
                <div>
                  <span t-if="o.type == 'out_refund'">
                    NOTA DE CRÉDITO-DÉBITO
                  </span>
                  <span t-if="o.type != 'out_refund'">
                    <t t-if="o.cc_dos.siat_sector_id.code == 1">
                      FACTURA
                    </t>
                    <t t-else="">
                      <span
                        t-esc="o.cc_dos.siat_sector_id.title or o.cc_dos.siat_sector_id.name or 'FACTURA'" />
                    </t>
                  </span>
                </div>
                <div>
                  <span t-if="o.cc_dos.siat_sector_id.tipo == 1">
                    - CON DERECHO A CRÉDITO FISCAL -
                  </span>
                  <span t-if="o.cc_dos.siat_sector_id.tipo == 2">
                    - SIN DERECHO A CRÉDITO FISCAL -
                  </span>
                </div>
              </strong>
            </div>
            <br />
            <br />

            <!-- row 3: issuer refs -->
            <div id="page_row3" class="text-center"
              style="font-size:25px;">
              <ul style="list-style: none; line-height: 26px;">
                <li>
                  <strong>
                    <span t-field="o.company_id.name" />
                  </strong>
                </li>
                <br />
                <t t-if="o.cc_dos.sucursal == 0">
                  <li>
                    <span class="normal">CASA MATRIZ</span>
                  </li>
                </t>
                <t t-if="o.cc_dos.sucursal != 0">
                  <li>
                    <span class="normal">SUCURSAL N. 0</span>
                    <span t-field="o.cc_dos.sucursal" />
                  </li>
                </t>
                <li>
                  <span class="normal">No. Punto de Venta </span>
                  <span t-field="o.cc_dos.pdv" />
                </li>
                <li>
                  <span t-field="o.company_id.street" />
                </li>
                <li>
                  <span t-field="o.company_id.street2" />
                </li>
                <t t-if="o.cc_dos.sucursal == 0">
                  <li> Tel.: <span t-field="o.cc_dos.company_id.phone" />
                  </li>
                  <li>
                    <span t-field="o.cc_dos.company_id.municipio" />
                  </li>
                </t>
                <t t-if="o.cc_dos.sucursal != 0">
                  <li> Tel.: <span t-field="o.cc_dos.warehouse_id.partner_id.phone" />
                  </li>
                  <li>
                    <span t-field="o.cc_dos.warehouse_id.partner_id.city" />
                  </li>
                </t>
              </ul>
            </div>

            <br />
            <div class="row zero_min_height" id="header_line2">
              <div class="col-xs-12">
                <div style="border-bottom: 6px dotted black;" />
              </div>
            </div>
            <br />

            <!-- row 4: invoice refs -->
            <div id="page_row4" class="text-center"
              style="margin-left:20px; margin-top:10px; margin-bottom:10px; font-size: 26px;">
              <strong>NIT</strong>
              <br />
              <span t-field="o.company_id.nit" />
              <br />
              <t t-if="o.type != 'out_refund'">
                <strong>FACTURA N°</strong>
                <br />
                <span t-esc="o.cc_nro" />
                <br />
              </t>
              <t t-if="o.type == 'out_refund'">
                <strong>NOTA N°</strong>
                <br />
                <span t-esc="o.cc_nro" />
                <br />
              </t>
              <strong class="normal">COD. AUTORIZACIÓN</strong>
              <br />
              <span class="normal" t-esc="o.cuf[0:10]" />
              <span class="normal" t-esc="o.cuf[10:33]" />
              <span class="normal" t-esc="o.cuf[33:]" />
            </div>

            <br />
            <div class="row zero_min_height" id="header_line3">
              <div class="col-xs-12">
                <div style="border-bottom: 6px dotted black;" />
              </div>
            </div>
            <br />

            <!-- row 5: partner/date refs -->
            <table id="patner_ref_table" class="table table-condensed"
              style="font-size: 22px;">
              <tbody id="partner_ref_list">
                <tr>
                  <td style="text-align: right; font-weight: bold;">
                    Nombre/Razón Social:
                  </td>
                  <td style="text-align: left;">
                    <span t-field="o.razon" />
                  </td>
                </tr>
                <t t-if="o.siat_nombre_estudiante and o.siat_nombre_estudiante != ''">
                  <tr>
                    <td style="text-align: right; font-weight: bold;">
                      <strong>Nombre Estudiante:</strong>
                    </td>
                    <td style="text-align: left;">
                      <span t-esc="o.siat_nombre_estudiante" />
                    </td>
                  </tr>
                </t>
                <tr>
                  <td style="text-align: right; font-weight: bold;">
                    NIT/CI/CEX:
                  </td>
                  <td style="text-align: left;">
                    <span t-field="o.nit" />
                    <t t-if="o.siat_tipo_id.code == 1">
                      <span t-esc="' '" />
                      <span t-field="o.partner_id.extension" />
                    </t>
                  </td>
                </tr>
                <tr>
                  <td style="text-align: right; font-weight: bold;">
                    Cod. Cliente:
                  </td>
                  <td style="text-align: left;">
                    <span t-field="o.nit" />
                  </td>
                </tr>
                <t t-if="o.siat_periodo_facturado and o.siat_periodo_facturado != ''">
                  <tr>
                    <td style="text-align: right; font-weight: bold;">
                      Periodo Facturado:
                    </td>
                    <td style="text-align: left;">
                      <span t-esc="o.siat_periodo_facturado" />
                    </td>
                  </tr>
                </t>
                <tr>
                  <td style="text-align: right; font-weight: bold;">
                    Fecha de Emision:
                  </td>
                  <td style="text-align: left;">
                    <span
                      t-esc="time.strftime('%d/%m/%Y  %H:%M', time.strptime(o.time_stamp.replace('T', ' ')[0:16], '%Y-%m-%d %H:%M'))" />
                  </td>
                </tr>
              </tbody>
            </table>

            <div class="row zero_min_height" id="header_line4">
              <div class="col-xs-12">
                <div style="border-bottom: 6px dotted black;" />
              </div>
            </div>
            <br />

            <!-- row 6: out_refund -->
            <div t-if="(o.type == 'out_refund')" id="page_row6">

              <!-- legacy -->
              <t t-if="o.legacy_id">
                <t t-foreach="o.legacy_id" t-as="ir">
                  <div class="" t-if="(o.type == 'out_refund')"
                    style="font-size:14px;">
                    <div class="col-12"
                      style="padding-bottom:0px;margin-bottom:4px;">
                      <div class="panel panel-default">
                        <div class="panel-body">
                          <div class="row">
                            <div class="col-4">
                              <p class="small">Factura N°: <span
                                  t-field="ir.cc_nro" />
                              </p>
                            </div>
                            <div class="col-4">
                              <p class="small">N° Autorización/CUF: <span
                                  t-field="ir.cc_aut" />
                              </p>
                            </div>
                            <div class="col-4">
                              <p class="small">Fecha Factura: <span
                                  t-field="ir.date_emit" />
                              </p>
                            </div>
                          </div>
                          <div class="panel-header"
                            style="padding:0px;margin:0px;">
                            <p class="text-center">
                              <strong>DATOS FACTURA ORIGINAL</strong>
                            </p>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </t>
              </t>

              <!-- not legacy -->
              <t t-if="not o.legacy_id">
                <div class="row" t-if="(o.type == 'out_refund')"
                  style="font-size:14px;">
                  <div class="col-12"
                    style="padding-bottom:0px;margin-bottom:4px;">
                    <div class="panel panel-default">

                      <div class="panel-body">
                        <div class="row">
                          <div class="col-4">
                            <p class="small">Factura N°: <span
                                t-field="o.refunds_id.cc_nro" />
                            </p>
                          </div>
                          <div class="col-4">
                            <p class="small">Fecha Factura: <span
                                t-if="o.refunds_id.time_stamp == False"
                                t-esc="time.strftime('%d/%m/%Y  %H:%M', time.strptime(str(o.refunds_id.invoice_date) + ' 08:00', '%Y-%m-%d %H:%M'))"
                              />
                                                            <span
                                t-if="o.refunds_id.time_stamp != False"
                                t-esc="time.strftime('%d/%m/%Y  %H:%M', time.strptime(o.refunds_id.time_stamp.replace('T', ' ')[0:16], '%Y-%m-%d %H:%M'))"
                              />
                                                            <span
                                t-field="o.refunds_id.cc_aut" />
                            </p>
                          </div>
                          <div class="col-4">
                            <p class="small">N° Autorización/CUF: <span
                                t-field="o.refunds_id.cc_aut"
                                t-if="o.refunds_id.cc_aut != '0'" />
                              <t
                                t-if="o.refunds_id.cuf and len(o.refunds_id.cuf) > 34">
                                <span class="normal"
                                  t-esc="o.refunds_id.cuf[0:10]" />
                                <span class="normal"
                                  t-esc="o.refunds_id.cuf[10:33]" />
                                <span class="normal"
                                  t-esc="o.refunds_id.cuf[33:]" />
                              </t>
                            </p>
                          </div>
                        </div>
                      </div>
                      <div class="panel-header"
                        style="padding:0px;margin:0px;">
                        <p class="text-center">
                          <strong>DATOS FACTURA ORIGINAL</strong>
                        </p>
                      </div>
                    </div>
                  </div>
                </div>
              </t>
            </div>

            <!-- row 7: out_refund tipo_fac 6 -->
            <div t-if="(o.type == 'out_refund' and o.tipo_fac == '6')"
              id="page_row7">
              <div class="row" t-if="(o.type == 'out_refund')" style="font-size:14px;">

                <!-- legacy -->
                <t t-if="o.legacy_id">
                  <t t-foreach="o.legacy_id" t-as="ir2">
                    <div class="col-12">
                      <div class="panel panel-default">
                        <table class="table table-sm" id="invoice_list_1">
                          <thead>
                            <tr>
                              <th>Código</th>
                              <th>Descripción</th>
                              <th>Cantidad</th>
                              <th>Unidad de Medida</th>
                              <th class="text-right">Precio Unitario</th>
                              <th class="text-right">Descuento (Bs)</th>
                              <th class="text-right">Subtotal (Bs)</th>
                            </tr>
                          </thead>
                          <tbody class="invoice_tbody">
                            <tr t-foreach="ir2.invoice_line" t-as="irl">
                              <td>
                                <span
                                  t-field="irl.product_id.default_code" />
                              </td>
                              <td>
                                <span t-field="irl.product_id" />
                              </td>
                              <td>
                                <span t-esc="irl.quantity" />
                              </td>
                              <td>
                                <span
                                  t-esc="irl.product_id.siat_unidad_medida_id.name" />
                              </td>
                              <td class="text-right">
                                <span
                                  t-esc="'{0:,.2f}'.format(round(irl.price_unit, 2))" />
                              </td>
                              <td class="text-right">
                                <span
                                  t-esc="'{0:,.2f}'.format(round((irl.price_unit * irl.quantity) * irl.discount / 100, 2))" />
                              </td>
                              <td class="text-right">
                                <span
                                  t-esc="'{0:,.2f}'.format(round(irl.price_subtotal, 2))" />
                              </td>
                            </tr>
                          </tbody>
                        </table>
                        <div class="row" id="invoice_total_refund">
                          <div class="col-12">
                            <table class="table table-sm" id="total_1">
                              <tr>
                                <td style="text-indent: 10px">
                                  <strong>SON :</strong>
                                  <span
                                    t-esc="o.to_word(ir2.amount_total)" />
                                  <strong> Bs.</strong>
                                </td>
                                <td class="text-right">
                                  <strong>MONTO TOTAL ORIGINAL Bs</strong>
                                </td>
                                <td class="text-right active">
                                  <span
                                    t-esc="'{0:,.2f}'.format(ir2.amount_total)" />
                                </td>
                              </tr>
                            </table>
                          </div>
                        </div>
                      </div>
                    </div>
                  </t>
                </t>

                <!-- not legacy -->
                <t t-if="not o.legacy_id">
                  <div class="col-12" style="font-size:14px;">
                    <div class="panel panel-default">
                      <table class="table table-sm" id="invoice_lines_refund">
                        <thead>
                          <tr>
                            <th>Código</th>
                            <th>Descripción</th>
                            <th>Cantidad</th>
                            <th>Unidad de Medida</th>
                            <th class="text-right">Precio Unitario</th>
                            <th class="text-right">Descuento (Bs)</th>
                            <th class="text-right">Subtotal (Bs)</th>
                          </tr>
                        </thead>
                        <tbody class="invoice_tbody">
                          <tr t-foreach="o.refunds_id.invoice_line_ids"
                            t-as="l">
                            <td>
                              <span
                                t-field="l.product_id.default_code" />
                            </td>
                            <td>
                              <span t-field="l.product_id" />
                            </td>
                            <td>
                              <span t-esc="l.quantity" />
                            </td>
                            <td>
                              <span
                                t-esc="l.product_id.siat_unidad_medida_id.name" />
                            </td>
                            <td class="text-right">
                              <span
                                t-esc="'{0:,.2f}'.format(round(l.price_unit, 2))" />
                            </td>
                            <td class="text-right">
                              <span
                                t-esc="'{0:,.2f}'.format(l.price_unit * l.quantity * l.discount / 100)" />
                            </td>
                            <td class="text-right">
                              <span
                                t-esc="'{0:,.2f}'.format(l.price_subtotal_with_tax)" />
                            </td>
                          </tr>
                        </tbody>
                      </table>
                      <div class="row" id="invoice_total_refund">
                        <div class="col-12">
                          <table class="table table-sm" id="total_refund">
                            <tr>
                              <td style="text-indent: 10px">
                                <strong>SON :</strong>
                                <span
                                  t-esc="o.to_word(o.refunds_id.amount_total)" />
                                <strong> Bs.</strong>
                              </td>
                              <td class="text-right">
                                <strong>MONTO TOTAL ORIGINAL Bs</strong>
                              </td>
                              <td class="text-right active">
                                <span
                                  t-esc="'{0:,.2f}'.format(o.refunds_id.amount_total)" />
                              </td>
                            </tr>
                          </table>
                        </div>
                      </div>
                    </div>
                  </div>
                </t>
              </div>
            </div>

            <!-- row 8: out_refund data subtitle -->
            <div class="row" t-if="(o.type == 'out_refund')" id="page_row8">
              <div class="col-12" style="padding-bottom:0px;margin-bottom:4px;">
                <div class="panel panel-default"
                  style="padding-bottom:0px;margin-bottom:4px;">
                  <div class="panel-header">
                    <p class="text-center">
                      <strong>DATOS DE LA DEVOLUCIÓN O RESCISIÓN</strong>
                    </p>
                  </div>
                </div>
              </div>
            </div>

            <!-- row 9: invoice table -->
            <table id="invoice_lines_original" class="table table-condensed"
              style="font-size: 24px;">
              <tbody id="invoice_list_3" class="invoice_tbody">
                <t t-foreach="o.invoice_line_ids" t-as="l">
                  <tr style="line-height: 18px; margin-left:-5px;">
                    <td colspan="5">
                      <span t-field="l.name" />
                    </td>
                  </tr>
                  <tr style="line-height: 18px; margin-left:-5px;">
                    <td>

                    </td>
                    <td class="text-center">
                      <span t-esc="l.quantity" />
                      <span
                        t-field="l.uom_id.siat_unidad_medida_id.name" />
                    </td>
                    <td class="text-right"> x <span t-field="l.price_unit"
                        t-field-options="{&quot;widget&quot;: &quot;monetary&quot;, &quot;display_currency&quot;: &quot;o.currency_id&quot;}" />
                    </td>
                    <td class="text-right"> - <span t-esc="'{0:,.2f}'.format(l.discount)" />
                    </td>
                    <td class="text-right" style="font-weight: bold;">
                      <span
                        t-esc="'{0:,.2f}'.format(l.price_subtotal_with_tax)" />
                    </td>
                  </tr>
                </t>
              </tbody>
            </table>

            <!-- totals -->
            <div class="row" id="invoice_total_orginal">
              <div class="text-right" style="width: 99%;">
                <t t-set="sum_sin_desc"
                  t-value="o.amount_total + o.sum_desc" />
                <table class="table" style="font-size: 25px;">
                  <tr
                    t-if="(o.type != 'out_refund')">
                    <td class="text-right">
                      <strong>SUBTOTAL Bs.</strong>
                    </td>
                    <td class="text-right">
                      <span t-esc="'{0:,.2f}'.format(o.total_bs)" />
                    </td>
                  </tr>
                  <tr
                    t-if="(o.type != 'out_refund')">
                    <td class="text-right">
                      <strong>DESCUENTO Bs.</strong>
                    </td>
                    <td class="text-right">
                      <span
                        t-esc="'{0:,.2f}'.format(o.siat_discount)" />
                    </td>
                  </tr>
                  <tr>
                    <td class="text-right">
                      <strong t-if="(o.type != 'out_refund')">TOTAL
                        Bs.</strong>
                      <strong t-if="(o.type == 'out_refund')">MONTO
                        TOTAL DEVUELTO Bs.</strong>
                    </td>
                    <td class="text-right">
                      <span
                        t-esc="'{0:,.2f}'.format(o.total_bs - o.siat_discount)" />
                    </td>
                  </tr>
                  <tr
                    t-if="(o.type != 'out_refund' and o.siat_sector_code not in [2, 11])">
                    <td class="text-right">
                      <strong>MONTO GIFT CARD Bs.</strong>
                    </td>
                    <td class="text-right">
                      <span t-esc="'{0:,.2f}'.format(o.giftcard)" />
                    </td>
                  </tr>
                  <tr
                    t-if="(o.type != 'out_refund' and o.siat_sector_code not in [2, 11])">
                    <td class="text-right">
                      <strong>MONTO A PAGAR Bs.</strong>
                    </td>
                    <td class="text-right">
                      <span
                        t-esc="'{0:,.2f}'.format(o.residual - o.siat_discount - o.giftcard)" />
                    </td>
                  </tr>
                  <tr
                    t-if="o.cc_dos.siat_sector_id.tipo == 1">
                    <td class="text-right">
                      <strong>IMPORTE BASE CRÉDITO FISCAL Bs.</strong>
                    </td>
                    <td class="text-right">
                      <span
                        t-esc="'{0:,.2f}'.format(o.total_bs - o.siat_discount - o.giftcard)" />
                    </td>
                  </tr>
                  <tr
                    t-if="(o.type == 'out_refund')">
                    <td class="text-right">
                      <strong>MONTO EFECTIVO DÉBITO-CRÉDITO Bs</strong>
                    </td>
                    <td class="text-right">
                      <span
                        t-esc="'{0:,.2f}'.format((o.total_bs - o.siat_discount) * 0.13)" />
                    </td>
                  </tr>
                </table>
              </div>
            </div>
            <br />
            <br />
            <div class="row">
              <div>
                <table class="table table-sm"
                  style="font-size: 25px;"
                  id="total_3">
                  <tr>
                    <td style="text-indent: 20px; text-align: right;">
                      <strong>Son: </strong>
                      <span
                        t-esc="o.to_word(o.total_bs - o.siat_discount)" />
                      <!-- <strong> Bolivianos</strong> -->
                    </td>
                  </tr>
                </table>
              </div>
            </div>

            <br />
            <div class="row zero_min_height" id="header_line5">
              <div class="col-xs-12">
                <div style="border-bottom: 6px dotted black;" />
              </div>
            </div>
            <br />
            <br />

            <!-- footer ref -->
            <!-- <div id="footer_reference"> -->
            <div id="footer_ref" class="row text-center"
              style="font-size: 24px; line-height: 25px; margin-left:1px; width:100%;">
              <para
                t-if="((o.type == 'out_invoice' or o.type == 'out_refund') and ((o.state == 'open') or (o.state == 'paid')))">
                <b>ESTA FACTURA CONTRIBUYE AL DESARROLLO DEL
                  PAÍS, EL USO ILÍCITO SERÁ SANCIONADO
                  PENALMENTE DE ACUERDO A LEY</b>
                <br />
                <br />
                <span class="small" t-field="o.siat_leyenda" />
                <br />
                <br />
                <t t-if="o.siat_modalidad != '2'">
                  <b>"Este documento es la Representación
                    Gráfica de un Documento Fiscal
                    Digital emitido en una modalidad de
                    facturación en línea"</b>
                  <br />
                  <br />
                </t>
                <t t-if="o.siat_modalidad == '2'">
                  <b>“Este documento es la Representación
                    Gráfica de un Documento Fiscal
                    Digital emitido fuera de línea,
                    verifique su envío con su proveedor
                    o en la página web
                    www.impuestos.gob.bo”</b>
                  <br />
                  <br />
                </t>
              </para>
              <para style="terp_default_Bold_9"
                t-if="((o.type == 'out_invoice' or o.type == 'out_refund') and (o.state == 'draft' or o.state == 'cancel'))">
                "ESTE DOCUMENTO NO TIENE VALIDEZ FISCAL" <br />
                <br />
              </para>
            </div>
            <div style="width:100%; text-align: center;">
              <span
                t-if="((o.type == 'out_invoice' or o.type == 'out_refund') and ((o.state == 'open') or (o.state == 'paid')))">
                <img t-if="o.siat_link"
                  t-att-src="'/report/barcode/?type=%s&amp;width=%s&amp;height=%s&amp;value=%s' % ('QR', 420, 420, o.siat_link_url)"
                  alt="QR Code" style="height:425px" />
              </span>
            </div>

            <!-- row 10: invoice type B -->
            <div class="row" id="page_row10">
              <div class="col-xs-6" id="invoice_ref">
                <span
                  t-if="o.type == 'out_invoice' and ((o.state == 'open') or (o.state == 'paid'))"></span>
                <span t-field="o.name" />
              </div>
              <div t-if="o.type == 'out_refund'" class="col-xs-6">
                <span t-if="o.refunds_id.nit" class="small">
                  <strong>Factura Original:</strong>
                  <span t-field="o.refunds_id.nit" />
                </span>
              </div>
              <div t-if="o.type == 'out_refund'" class="col-xs-6">
                <span t-if="o.cc_nro" class="small">
                  <strong>Factura rectificativa:</strong>
                  <span t-field="o.cc_nro" />
                </span>
              </div>
            </div>
          </div>

          <!-- END PAGE -->

          <!-- footer -->
          <div id="footer" style="text-align: center; font-size:10px;"> ID: <span t-field="o.id" />
            IUID: <span t-field="o.user_id.id" /> PUID: <span t-field="o.env.user.id" /> PDATE: <span
              t-esc="context_timestamp(datetime.datetime.now()).strftime('%Y-%m-%d %H:%M:%S')" />
          </div>

        </t>
      </t>
    </template>

  </data>
</odoo>