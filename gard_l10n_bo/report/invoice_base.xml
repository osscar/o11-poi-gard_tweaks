<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data>
    <template id="report_account_invoice_base_bol_siat"
      inherit_id="account.report_invoice_with_payments">
      <xpath expr="//t/t" position="replace">
        <t name="base" t-if="report == 'invoice' and inv_mode in ['1','2','0']"
          t-call="gard_l10n_bo.siat_invoice" />
        <t name="base" t-if="report == 'invoice' and inv_mode == '-1'"
          t-call="gard_l10n_bo.basic_invoice" />
      </xpath>
    </template>
  </data>
</odoo>