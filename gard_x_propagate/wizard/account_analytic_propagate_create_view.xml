<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>

    <record id="view_account_analytic_propagate_create_form" model="ir.ui.view">
      <field name="name">account.analytic.propagate.create.form</field>
      <field name="model">account.analytic.propagate.create</field>
      <field name="arch" type="xml">
        <form string="Create Analytic Accounts">
          <group name="create_form">
            <field name="account_analytic_propagate_group" />
            <field name="account_analytic_default_id" />
            <!-- domain="[('type', 'in', type)]"/> -->
          </group>
          <group name="create_form_lines">
            <notebook>
              <page name="create_values" string="Create Values">
                <button name="button_wizard_line_unlink" type="object"
                  attrs="{'invisible':[('account_analytic_default_ids', '=', [])]}"
                  class="btn-default" string="Clear Lines" />
                <separator />
                <field name="account_analytic_default_ids">
                  <tree>
                    <field name="parent_id" />
                    <field name="order_name" />
                    <field name="name" />
                    <field name="code" />
                  </tree>
                </field>
              </page>
            </notebook>
          </group>
          <footer>
            <button string="Create" name="button_create" type="object" class="btn-primary" />
            <button string="Cancel" class="btn-default" special="cancel" />
          </footer>
        </form>
      </field>
    </record>

    <record id="action_view_account_analytic_propagate_create" model="ir.actions.act_window">
      <field name="name">Create Analytic Accounts</field>
      <field name="res_model">account.analytic.propagate.create</field>
      <field name="view_type">form</field>
      <field name="view_mode">tree,form</field>
      <field name="view_id" ref="view_account_analytic_propagate_create" />
      <field name="target">new</field>
    </record>

  </data>
</odoo>