<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Sequence for supplier invoice refund requests --> 
        <record id="sequence_account_invoice_in_refund_request" model="ir.sequence">
            <field name="name">Supplier Invoice Refund Request</field>
            <field name="code">account.invoice.in.refund.request</field>
            <field name="prefix">RFP/%(year)s.%(month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for customer invoice refund requests --> 
        <record id="sequence_account_invoice_out_refund_request" model="ir.sequence">
            <field name="name">Customer Invoice Refund Request</field>
            <field name="code">account.invoice.out.refund.request</field>
            <field name="prefix">RFC/%(year)s.%(month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
