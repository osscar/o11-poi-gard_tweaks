<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="group_account_edit" model="res.groups">
            <field name="name">GARD Accounting Edit</field>
            <field name="category_id" ref="base.module_category_usability"/>
            <field name="comment">GARD custom group for special permission to edit information in posted records.</field>
            <field name="implied_ids" eval="[(4, ref('base.group_no_one'))]"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/>
        </record>

        <record id="group_cashier" model="res.groups">
            <field name="name">GARD Cashier</field>
            <field name="category_id" ref="base.module_category_usability"/>
            <field name="comment">GARD custom group for cashier.</field>
            <field name="implied_ids" eval="[(4, ref('base.group_no_one'))]"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/>
        </record>

    </data>
</odoo>
