<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record forcecreate="True" id="ir_cron_goup_account_edit_reset_users" model="ir.cron">
            <field name="name">Group: Account Edit (Reset Users)</field>
            <field name="model_id" ref="base.model_res_groups"/>
            <field name="state">code</field>
            <field name="code">
group_aced = env.ref('gard_x_gard.group_account_edit')
res_group = model.search([('id','=',group_aced.id)])
res_group['users'] = False
                </field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">12</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field eval="True" name="doall"/>
        </record>

    </data>
</odoo>
