<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- web_google_maps customizations -->
    <record id="view_stock_picking_map" model="ir.ui.view">
        <field name="name">view.stock.picking.map</field>
        <field name="model">stock.picking</field>
