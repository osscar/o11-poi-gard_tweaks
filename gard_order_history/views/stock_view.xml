<?xml version="1.0"?>
<openerp>
    <data>

        <record id="vpicktree" model="ir.ui.view">
            <field name="name">vpicktree.inherit.gard_order_history</field>
            <field name="model">stock.picking</field>
            <field name="mode">extension</field>
            <field name="inherit_id" ref="stock.vpicktree"/>
            <field name="arch" type="xml">

                <xpath expr="//tree[@string='Picking list']" position="replace">
                    <tree colors="blue:state == 'draft';gray:state == 'cancel';red:state not in ('cancel', 'done') and min_date &lt; current_date;mediumseagreen: state == 'done'">
                        <field name="state"/>
                        <field name="min_date"/>
                        <field name="date_done"/>
                        <field name="name"/>
                        <field name="group_id"/>
                        <field name="origin"/>
                        <field name="partner_id"/>
                        <field name="location_id"/>
                        <field name="location_dest_id"/>
                        <field name="carrier_id"/>
                        <field name="backorder_id"/>
                        <field name="create_uid"/>
                        <field invisible="1" name="create_date"/>
                        <field name="priority" invisible="1"/>
                        <field name="picking_type_id" invisible="1"/>
                    </tree>
                </xpath>

            </field>
        </record>

        <record id="stock_picking_view_tree" model="ir.ui.view">
            <field name="name">stock.picking.tree.gard_order_history</field>
            <field name="model">stock.picking</field>
            <field name="view_mode">tree</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">

                <tree colors="blue:state == 'draft';gray:state == 'cancel';red:state not in ('cancel', 'done') and min_date &lt; current_date;mediumseagreen: state == 'done'">
                    <field name="state"/>
                    <field name="min_date"/>
                    <field name="date_done"/>
                    <field name="name"/>
                    <field name="origin"/>
                    <field name="partner_id"/>
                    <field name="location_id"/>
                    <field name="location_dest_id"/>
                </tree>

            </field>
        </record>

    </data>
</openerp>
