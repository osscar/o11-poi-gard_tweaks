<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_import_from_export_list_wizard_form" model="ir.ui.view">
        <field name="name">import.from.export.list.wizard.form</field>
        <field name="model">import.from.export.list.wizard</field>
        <field name="arch" type="xml">
            <form string="Import Data from Export List">
                <group>
                    <field name="model_id" required="1" />
                    <field name="export_list_id" required="1" />
                </group>
                <footer>
                    <button name="export_csv_template" string="Export CSV Template" type="object" class="btn-primary"/>
                    <button name="import_data" string="Import Data" type="object"
                        class="btn-primary" />
                    <button string="Cancel" class="btn-secondary" special="cancel" />
                </footer>
            </form>
        </field>
    </record>

    <act_window id="action_import_from_export_list_wizard" name="Import Data from Export List"
        res_model="import.from.export.list.wizard" view_mode="form" target="new" />
</odoo>